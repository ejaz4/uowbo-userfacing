generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DiscordUser {
  id               String         @id @default(uuid())
  discordId        String         @unique
  university       UniversityUser @relation(fields: [universityUserId], references: [id])
  universityUserId String
}

model UniversityUser {
  id          String        @id @default(uuid())
  email       String        @unique
  name        String
  DiscordUser DiscordUser[]
}

model Handover {

}
